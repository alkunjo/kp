<div class="container" style="margin: 10px 10px 10px 10px">
	<div class="row">
		<div class="col-md-1">Nama</div>
		<div class="col-md-8">: <%= @user.user_name %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Username</div>
		<div class="col-md-8">: <%= @user.user_username %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Telepon</div>
		<div class="col-md-8">: <%= @user.user_phone %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Alamat</div>
		<div class="col-md-8">: <%= @user.user_address %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Email</div>
		<div class="col-md-8">: <%= @user.email %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Position</div>
		<div class="col-md-8">: <%= @user.position.position_name %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Outlet</div>
		<div class="col-md-8">: <%= @user.outlet.outlet_name %></div>
	</div>
	<div class="row">
		<div class="col-md-1">Role</div>
		<div class="col-md-8">: <%= @user.role.role_name %></div>
	</div>
	<div class="row">
		<div class="col-md-2">Last Sign In</div>
		<div class="col-md-8">: <%= @user.last_sign_in_at %></div>
	</div>	
</div>