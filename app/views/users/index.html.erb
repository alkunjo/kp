<p id="notice"><%= notice %></p>

<h3 align="center"><b>Master User</b></h3><br/><br/>

<div id="form" style="max-width:1000px; min-height:100px">
  <%= render 'form' %>
</div>

<%= form_tag import_users_path, multipart: true do %>
  <div class="col-md-3"><b>Import User</b></div>
  <div class="col-md-4">
    <%= file_field_tag :file, class: "form-control" %>
  </div>
  <div class="col-md-4">
    <%= submit_tag "Submit", class: "btn btn-sm btn-primary" %>
  </div> 
<% end %>

<br/><br/><br/>

<table class="table table-striped table-bordered">
  <thead>
    <tr align="center">
      <th style="text-align:center">Nama</th>
      <th style="text-align:center">Posisi</th>
      <th style="text-align:center">Outlet</th>
      <th style="text-align:center">Role</th>
      <th style="text-align:center">Telepon</th>
      <th style="text-align:center">Last Sign In</th>
      <th colspan="2" style="text-align:center">Aksi</th>
    </tr>
  </thead>

  <tbody id="tabelUser" class="isiUser">
    <%= render 'index' %>
  </tbody>
</table>
<div class="modal fade" id="userModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog"style="width:450px">
    <div class="modal-content">
      <div class="modal-header" align="center">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel"></h4>
      </div>
      <div class="modal-isi"></div>
    </div>
  </div>
</div>