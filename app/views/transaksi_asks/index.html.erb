<p id="notice"><%= notice %></p>

<h3 align="center"><b>Transaksi Permintaan Obat</b></h3><br/><br/>

<div id="form" style="max-width:1000px; min-height:70px">
  <%= render 'form' %>
</div>

<table class="table table-striped table-bordered">
  <thead>
    <tr align="center">
      <th style="text-align:center">Apotek Tujuan</th>
      <th style="text-align:center">Tanggal</th>
      <th style="text-align:center">Status</th>
      <th colspan="2" style="text-align:center">Aksi</th>
    </tr>
  </thead>

  <tbody id="tabelTransAsk" class="isiTransAsk">
    <%= render 'index' %>
  </tbody>
</table>
<div class="modal fade" id="transAskModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog"style="max-width:700px">
    <div class="modal-content">
      <div class="modal-header" align="center">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel"></h4>
      </div>
      <div class="modal-isi"></div>
    </div>
  </div>
</div>
<div class="modal fade" id="kpModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog"style="width:450px">
      <div class="modal-content">
        <div class="modal-header" align="center">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="myModalLabel"></h4>
        </div>
        <div class="modal-isi"></div>
      </div>
    </div>
  </div>